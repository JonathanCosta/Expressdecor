<div id="user_task_list_wrapper">
    <div id="user_task_list">
        <div id="user_task_list_content">
            <h3>My Task List</h3><a href="#" id="minimise-tasks-list">maximize</a>
            <div class="clear"></div>
                <div style="padding:0 0 10px; text-align: right;">
                    Filter tasks by priority:                    <select id="prioritySelectah">
                        <option value="any">any</option>
                        <option value="1">1</option><option value="2">2</option><option value="3">3</option><option value="4">4</option><option value="5">5</option>                    </select>
