<?xml version="1.0"?>
<config>
    <modules>
        <Expressdecor_Page>
            <version>0.1.0</version>
        </Expressdecor_Page>
    </modules>
    <frontend>
        <routers>
            <page>
                <use>standard</use>
                <args>
                    <module>Expressdecor_Page</module>
                    <frontName>page</frontName>
                </args>
            </page>
        </routers> 
    </frontend>
 <global>
        <blocks>
           <page>
              <rewrite>
                  <html_head>Expressdecor_Page_Block_Html_Head</html_head> 
               </rewrite>
           </page>
        </blocks>           
  </global>
</config>


	